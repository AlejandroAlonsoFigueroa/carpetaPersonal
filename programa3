
using System;
class HelloWorld {
  static void Main() {
    char letra;
    Console.WriteLine("INgresa una letra");
    letra = Convert.ToChar(Console.ReadLine());
    
    switch(letra){
        case 'a':
            Console.WriteLine("Es vocal");
            break;
        case 'e':
            Console.WriteLine("Es vocal");
            break;
        case 'i':
            Console.WriteLine("Es vocal");
            break;
        case 'o':
            Console.WriteLine("Es vocal ");
            break;
        case 'u':
            Console.WriteLine("Es vocal");
            break;
        default:
            Console.WriteLine("Es consonante");
            break;
       
        
        
        
    }
    
  }
}
